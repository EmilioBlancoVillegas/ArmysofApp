<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ArmySOF.Views.HomePage"
             Title="Inicio">
    <ContentPage.ToolbarItems>
        <ToolbarItem Text="Atrás" Activated="BackButton_Clicked"/>
        <ToolbarItem Text="Delante" Activated="ForwardButton_Clicked"/>
    </ContentPage.ToolbarItems>
    <ContentPage.Content>
        <StackLayout>
            <WebView x:Name="webView" WidthRequest="1000" HeightRequest="1000"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>